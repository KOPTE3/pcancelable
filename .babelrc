{
  "env": {
    "commonjs": {
      "plugins": [
        "transform-class-properties"
      ],
      "presets": [
        ["env", {
          "targets": { "ie": 9 }
        }]
      ]
    },
    "es": {
      "plugins": [
        "transform-class-properties"
      ],
      "presets": [
        ["env", {
          "modules": false,
          "targets": { "ie": 9 }
        }]
      ]
    },
    "test": {
      "plugins": [
        "transform-class-properties"
      ],
      "presets": [
        ["env", {
          "targets": { "node": "current" }
        }]
      ]
    }
  },
  "plugins": [
    "transform-class-properties"
  ]
}
